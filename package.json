{
  "name": "ds-visualizer",
  "version": "1.0.5",
  "description": "Generate visualization of complex data structure on the fly",
  "main": "dist/index.js",
  "types": "dist/index.t.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "data structure",
    "visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dangchinh25/ds-visualizer.git"
  },
  "author": "Chinh Le",
  "license": "ISC",
  "dependencies": {
    "viz.js": "^1.3.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
